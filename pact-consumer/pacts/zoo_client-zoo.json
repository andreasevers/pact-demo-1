{
    "provider": {
        "name": "zoo"
    },
    "consumer": {
        "name": "zoo_client"
    },
    "interactions": [
        {
            "description": "GET /animals",
            "request": {
                "method": "GET",
                "path": "/animals"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "animals": [
                        {
                            "name": "cat"
                        },
                        {
                            "name": "dolphin"
                        },
                        {
                            "name": "sloth"
                        }
                    ]
                }
            },
            "providerStates": [
                {
                    "name": "3 animals"
                }
            ]
        }
    ],
    "metadata": {
        "pactSpecification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.22"
        }
    }
}